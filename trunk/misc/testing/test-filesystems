#!/usr/bin/perl -w

use strict;
use POSIX;
use Time::HiRes;
use Filesys::Df;

my $debug = 0;
my $mount_point = '/mnt/tmp';
my $report_path = '/tmp/_filesystem-test-report_';
my $temp_directory = '/mnt/tmpfs';
my $test_data_source = '/dev/zero';
my $vd_mount_point = '/mnt/tmpvd';
my $vd_size = 67108864;
my $partition = sprintf("%s/disk.img", $vd_mount_point);
my $test_data_sizes = # bytes -> count of files; 0 == calculate maximum
{
    1024*12 => 100,
    1048576 => 1,
    8388608 => 1,
    16777216 => 1,
    67108864 => 1
#   200000000 => 1
};
my $iterations = 3;
my $iteration = 0;
my $fs = 
{
#    'bfs'	=> { 'format' => "mkfs -t bfs ${partition}"		, 'mount' => "mount -t bfs -o loop ${partition} ${mount_point}"		, 'umount' => "umount ${mount_point}" },
#    'xfs'	=> { 'format' => "mkfs -t xfs -f ${partition}"		, 'mount' => "mount -t xfs -o loop ${partition} ${mount_point}"		, 'umount' => "umount ${mount_point}" },
#    'ext2'	=> { 'format' => "mkfs -t ext2 -F ${partition}"		, 'mount' => "mount -t ext2 -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" },
    'ext3'	=> { 'format' => "mkfs -t ext3 -F ${partition}"		, 'mount' => "mount -t ext3 -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" },
#    'ext4dev'	=> { 'format' => "mkfs -t ext3 ${partition}"		, 'mount' => "mount -t ext3 -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" },
    'reiserfs'	=> { 'format' => "mkfs -t reiserfs -f -f ${partition}"	, 'mount' => "mount -t reiserfs -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" }
#    'reiser4'	=> { 'format' => "mkfs -t reiser4 -y ${partition}"	, 'mount' => "mount -t reiser4 -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" },
#    'fat32'	=> { 'format' => "mkfs -t vfat -F 32 ${partition}"	, 'mount' => "mount -t vfat -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" },
#    'tmpfs'	=> { 'format' => "true"					, 'mount' => "mount -t tmpfs tmpfs ${mount_point}"			, 'umount' => "umount ${mount_point}" },
#    'fat16'	=> { 'format' => "mkfs -t vfat -F 16 ${partition}"	, 'mount' => "mount -t vfat -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" },
#    'minix'	=> { 'format' => "mkfs -t minix ${partition}"		, 'mount' => "mount -t minix -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" },
#    'minix2'	=> { 'format' => "mkfs -t minix -v ${partition}"	, 'mount' => "mount -t minix -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" },
#    'ntfs'	=> { 'format' => "mkfs -t ntfs -F ${partition}"		, 'mount' => "mount -t ntfs -o loop,rw ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" }
#    'ntfs-3g'	=> { 'format' => "mkfs -t ntfs -F ${partition}"		, 'mount' => "mount -t ntfs-3g -o loop ${partition} ${mount_point}"	, 'umount' => "umount ${mount_point}" }
#    'zfs'	=> { 'format' => "zpool create testzfs ${partition} && zfs create testfs/first && zfs set mountpoint=${mount_point} testfs/first", 'mount' => "zfs mount", 'umount' => "zfs umount ${mount_point}", 'destroy' => "zfs destroy testfs/first && zpool destroy testfs", 'pre-bench' => '/etc/init.d/zfs-fuse start', 'post-bench' => '/etc/init.d/zfs-fuse stop' }
};

my $bench_data;
my $real_disk_size;

#my $gg = `which get_gradient 2>/dev/null`; chomp($gg);

sub debug_print
{
    my $line = $_[0];
    printf("[Debug] %s\n", $line);
}

sub prepare_fileset
{
    my $s_bench = [Time::HiRes::gettimeofday()];
    my $real_img_size = $vd_size + ceil(($vd_size/100)*30);
    my $real_vd_size = $real_img_size + (1048576);
    $real_disk_size = $real_vd_size;
    printf("Creating %db [%db real] tmpfs volume\n", $vd_size, $real_vd_size);
    system_command("mount -t tmpfs -o size=${real_vd_size} tmpfs ${vd_mount_point}") or return 0;
    system_command("dd if=/dev/zero of=${partition} bs=${real_img_size} count=1") or return 0;
    
    my $total_size = 0;
    foreach my $size (sort keys %{$test_data_sizes})
    {
	if($test_data_sizes->{$size} == 0)
	{
	    $test_data_sizes->{$size} = floor($vd_size/$size);
	}
	$total_size += $test_data_sizes->{$size}*$size;
	printf("Maximum %db files count on this ramdisk: %s\n", $size, floor($vd_size/$size));
    }
    printf("Total test-data size %db\n", $total_size);
    $total_size = $total_size + floor(($total_size/100)*10);
    printf("Creating %db tmpfs volume\n", $total_size);
    system_command("mount -t tmpfs -o size=${total_size} tmpfs ${temp_directory}") or return 0;
    foreach my $size (sort keys %{$test_data_sizes})
    {
	printf("Generating %s %s-size files. Total %s bytes\n", $test_data_sizes->{$size}, $size, $test_data_sizes->{$size}*$size);
	system_command("mkdir -p ${temp_directory}/${size}");
	for(my $i = 0; $i < $test_data_sizes->{$size}; $i++)
	{
	    system_command("dd if=${test_data_source} of=${temp_directory}/${size}/${i} bs=${size} count=1") or return 0;
	}
    }
    my $e_bench = [Time::HiRes::gettimeofday()];
    my $bench_diff = Time::HiRes::tv_interval($s_bench, $e_bench);
    printf("Time spent while creating files: %s\n", $bench_diff);
    return 1;
}

sub remove_fileset
{
    #system_command("rm -rf ${temp_directory}");
    system_command('sync && sync');
    system_command("umount ${temp_directory}");
    system_command("umount ${vd_mount_point}");
}

sub system_command
{
    my $command = $_[0];
    if($debug) { debug_print(sprintf("System command: %s", $command)); }
    my $result = system "${command} > /tmp/test-filesystems.out 2>&1";
    if($debug) { debug_print(sprintf("System command result: %d", $result)); }
    if($result != 0)
    {
	printf ("Command [%s] failed\n", $command);
	system "cat /tmp/test-filesystems.out";
    }
    return $result == 0;
}

sub bench_command
{
    my ($fs_name, $bench_name, $command, $caption) = @_[0..3];
    if($debug) { debug_print(sprintf("Bench command: %s", $command)); }
    my $log_filename = sprintf("%s/log/%s_%s_%s.log", $report_path, $fs_name, $bench_name, $iteration);
    my $real_command = sprintf("%s > %s 2>&1 && sync", $command, $log_filename);
    system_command('sync && sync');
    my $s_bench = [Time::HiRes::gettimeofday()];
    my $result = system $real_command;
    my $e_bench = [Time::HiRes::gettimeofday()];
    if($result == 0)
    {
	my $bench_diff = Time::HiRes::tv_interval($s_bench, $e_bench);
	append_bench_data('time', $fs_name, $caption, $bench_name, $bench_diff, $command);
    }
    else 
    {
	printf("Bench [%s] failed\n$!\n", $command);
	system "cat ${log_filename}";
    }
    if($debug) { debug_print(sprintf("Bench command result: %d", $result)); }
    return $result == 0;
}



sub bench_fs
{
    my $fs_name = $_[0];
    if(bench_command($fs_name, 'mount', $fs->{$fs_name}{'mount'}, "Mount time"))
    {
	append_bench_data('size', $fs_name, "After format", 'aformat', df($mount_point, 1));
	if( bench_command($fs_name, "mkdir", "mkdir -p ${mount_point}/a", "Make directory time") && 
	    bench_command($fs_name, "mkdir", "mkdir -p ${mount_point}/b", "Make directory time"))
	{
	    append_bench_data('size', $fs_name, "After making directoryes", 'amkdir', df($mount_point, 1));
	    foreach my $size (keys %{$test_data_sizes})
	    {
		my $job_name_tail = sprintf("%d_%d", $test_data_sizes->{$size}, $size);
		my $job_caption_tail = sprintf("%d files, %d bytes per file", $test_data_sizes->{$size}, $size);
		if(bench_command($fs_name, sprintf("copy_%s", $job_name_tail), "cp ${temp_directory}/${size}/* ${mount_point}/a", sprintf("Copying %s", $job_caption_tail)))
		{
		    append_bench_data('size', $fs_name, sprintf("After copying %s", $job_caption_tail), sprintf("acopy_%s", $job_name_tail), df($mount_point, 1));
		    if(bench_command($fs_name, sprintf("move_%s", $job_name_tail), "mv -f ${mount_point}/a/* ${mount_point}/b", sprintf("Moving %s", $job_caption_tail)))
		    {
			append_bench_data('size', $fs_name, sprintf("After moving %s", $job_caption_tail), sprintf("amove_%s", $job_name_tail), df($mount_point, 1));
			if(!bench_command($fs_name, sprintf("remove_%s", $job_name_tail), "rm -f ${mount_point}/b/*", sprintf("Removing %s", $job_caption_tail)))
			{
			    last;
			}
			append_bench_data('size', $fs_name, sprintf("After removing %s", $job_caption_tail), sprintf("aremove_%s", $job_name_tail), df($mount_point, 1));
		    }
		}
	    }
	}
	system_command('sync && sync');
	bench_command($fs_name, 'umount', $fs->{$fs_name}{'umount'}, "Unmount time");
    }
}

sub main
{
    my $s_bench = [Time::HiRes::gettimeofday()];
    system_command("mkdir -p ${report_path}/log");
    if(prepare_fileset())
    {
	for(; $iteration<$iterations; $iteration++)
	{
	    foreach my $fs_name ( sort keys %{$fs} )
	    {
		if(defined($fs->{$fs_name}{'pre-bench'})) { system_command($fs->{$fs_name}{'pre-bench'}); }
    		#printf("File system: %s\n", $fs_name);
		if(bench_command($fs_name, 'format', $fs->{$fs_name}{'format'}, "Formatting time"))
		{
		    system_command('sync && sync');
		    bench_fs($fs_name);
		    if(defined($fs->{$fs_name}{'destroy'})) { bench_command($fs_name, 'destroy', $fs->{$fs_name}{'destroy'}, "Destroy time"); }
		}
		if(defined($fs->{$fs_name}{'post-bench'})) { system_command($fs->{$fs_name}{'post-bench'}); }
	    }
	}
	make_averages_bench_data();
	$| = 1;
	print_result();
    }
    remove_fileset();
    my $e_bench = [Time::HiRes::gettimeofday()];
    my $bench_diff = Time::HiRes::tv_interval($s_bench, $e_bench);
    printf("Total time spent: %s\n", $bench_diff);
}

sub append_bench_data
{
    my ($type, $fs_name, $caption, $job_name) = @_[0..3];
    printf("Iteration %d, filesystem %s, task %s ", $iteration, $fs_name, $caption);
    $bench_data->{'captions'}{$job_name} = $caption;
    if($type eq 'size')
    {
	my ($size_data) = $_[4];
	printf("Used size: %d\n", $size_data->{'used'});
	my $spn_count = 0;
	if(!defined($bench_data->{'captions'}{$type}{$fs_name}{'names'})) { push(@{$bench_data->{'captions'}{$type}{$fs_name}{'names'}}, keys(%{$size_data})); }
	push(@{$bench_data->{'iterations'}{$type}{$fs_name}{$job_name}{'values'}[$iteration]}, values(%{$size_data}));
	foreach my $size_part_name (keys %{$size_data})
	{
	    if(!defined($size_data->{$size_part_name})) { next; }
	    
	    @{$bench_data->{'iterations'}{$type}{$fs_name}{$job_name}{'same_values'}{$size_part_name}}[$iteration] = $size_data->{$size_part_name};
	    
            if(defined(@{$bench_data->{'average'}{$type}{$fs_name}{$job_name}{'values'}{'summ'}}[$spn_count]))
            {
                @{$bench_data->{'average'}{$type}{$fs_name}{$job_name}{'values'}{'summ'}}[$spn_count] += $size_data->{$size_part_name};
            }
    	    else
            {
                push(@{$bench_data->{'average'}{$type}{$fs_name}{$job_name}{'values'}{'summ'}}, $size_data->{$size_part_name});
            }
	    $spn_count++;
	}
    }
    elsif ($type eq 'time')
    {
	my ($time_data, $command) = @_[4..5];
	printf("Time spent: %s\n", $time_data);
	push(@{$bench_data->{'iterations'}{$type}{$fs_name}{$job_name}{'values'}}, $time_data);
	$bench_data->{'iterations'}{$type}{$fs_name}{$job_name}{'command'} = $command;
	
	my $arr_cnt = 0;
	my $exists = 0;
	foreach my $arr_spn (@{$bench_data->{'average'}{$type}{$fs_name}{'names'}})
	{
	    if($arr_spn eq $job_name)
	    {
	        $exists = 1;
	        @{$bench_data->{'average'}{$type}{$fs_name}{'values'}{'count'}}[$arr_cnt]++;
	        @{$bench_data->{'average'}{$type}{$fs_name}{'values'}{'summ'}}[$arr_cnt] += $time_data;
	    }
	    $arr_cnt++;
	}
	    
	if(!$exists)
	{
	    push(@{$bench_data->{'average'}{$type}{$fs_name}{'names'}}, $job_name);
	    push(@{$bench_data->{'average'}{$type}{$fs_name}{'values'}{'count'}}, 1);
	    push(@{$bench_data->{'average'}{$type}{$fs_name}{'values'}{'summ'}}, $time_data);
	}
	
    }
}

sub make_averages_bench_data
{
    foreach my $type (sort keys %{$bench_data->{'average'}})
    {
	foreach my $fs_name (sort keys %{$bench_data->{'average'}{$type}})
	{
	    if($type eq 'time')
	    {
	        my $c_count = scalar(@{$bench_data->{'average'}{$type}{$fs_name}{'values'}{'count'}});
	        my $values;
	        for(my $i = 0; $i < $c_count; $i++)
	        {
	    	    push(@{$values}, @{$bench_data->{'average'}{$type}{$fs_name}{'values'}{'summ'}}[$i]/@{$bench_data->{'average'}{$type}{$fs_name}{'values'}{'count'}}[$i])
		}
		$bench_data->{'average'}{$type}{$fs_name}{'values'} = $values;
	    }
	    elsif($type eq 'size')
	    {
		foreach my $job_name (sort keys %{$bench_data->{'average'}{$type}{$fs_name}})
		{    
		    my $values;
		    my $c_count = scalar(@{$bench_data->{'average'}{$type}{$fs_name}{$job_name}{'values'}{'summ'}});
		    for(my $i = 0; $i < $c_count; $i++)
		    {
			push(@{$values}, @{$bench_data->{'average'}{$type}{$fs_name}{$job_name}{'values'}{'summ'}}[$i]/$iterations);
		    }
		    $bench_data->{'average'}{$type}{$fs_name}{$job_name}{'values'} = $values;
		}
	    }
	}
    }
}

use Data::Dumper;
use GD::Graph;
use GD::Graph::colour qw(:colours);
use GD::Graph::bars;
use GD::Graph::lines;
#use GD::Text;

sub print_result
{
    my $d = Data::Dumper->new([$bench_data]);
    $d->Indent(1);
    print $d->Dump; print "\n";
    my $index_fh = open_html_page('index');
    print ("Graph");
    print_time_result($index_fh);
    print_size_result($index_fh);
    print ("Done\n");
    close_html_page($index_fh);
}

sub print_time_result
{
    my ($index_page_fh) = $_[0];
    my $timep_fh = open_html_page('atime');
    print { $index_page_fh } "<br /><b>Times</b><br /><a href='atime.html'>Average times</a><br />\n";
    
    
    foreach my $fs_name ( keys %{$bench_data->{'average'}{'time'}})
    {
	my $names;
	foreach my $job_name (@{$bench_data->{'average'}{'time'}{$fs_name}{'names'}})
	{
	    push(@$names, $bench_data->{'captions'}{$job_name})
	}
	my $caption = sprintf("Average times of %s", $fs_name);
	my $img_filename = draw	(
				    'hbars',
				    sprintf("average_time_%s", $fs_name),
				    $caption,
				    'Names',
				    'Seconds',
				    $names,
				    $bench_data->{'average'}{'time'}{$fs_name}{'values'}
				);
	post_image($timep_fh, $img_filename, $caption);
    }
    close_html_page($timep_fh);
    $timep_fh = open_html_page('itime');
    print { $index_page_fh } "<a href='itime.html'>Every iteration times</a><br />\n";
    foreach my $fs_name ( keys %{$bench_data->{'iterations'}{'time'}})
    {
	foreach my $job_name ( keys %{$bench_data->{'iterations'}{'time'}{$fs_name}})
	{
	    my $caption = sprintf("Filesystem %s. %s", $fs_name, $bench_data->{'captions'}{$job_name});
	    my $img_filename = draw(
				    'lines',
				    sprintf("iterations_times_%s_%s", $fs_name, $job_name),
				    $caption,
				    'Names',
				    'Seconds',
				    'Iteration',
				    $bench_data->{'iterations'}{'time'}{$fs_name}{$job_name}{'values'}
				);
	    post_image($timep_fh, $img_filename, $caption, $bench_data->{'iterations'}{'time'}{$fs_name}{$job_name}{'command'});
	}
    }
    close_html_page($timep_fh);
}

sub print_size_result
{
    my ($index_page_fh) = $_[0];
    my $sizep_fh = open_html_page('afssize');
    print { $index_page_fh } "<br /><b>Sizes</b><br /><a href='afssize.html'>Average sizes per filesystem</a><br />\n";
    foreach my $fs_name ( keys %{$bench_data->{'average'}{'size'}})
    {
	foreach my $job_name ( keys %{$bench_data->{'iterations'}{'size'}{$fs_name}})
	{
	    my $caption = sprintf("Average sizes of %s. %s", $fs_name, $bench_data->{'captions'}{$job_name});
	    my $img_filename = draw	(
					    'hbars',
					    sprintf("average_size_%s_%s", $fs_name, $job_name),
					    $caption,
					    'Names',
					    'Bytes',
					    $bench_data->{'captions'}{'size'}{$fs_name}{'names'},
					    $bench_data->{'average'}{'size'}{$fs_name}{$job_name}{'values'}
					);
	    post_image($sizep_fh, $img_filename, $caption);
	}
    }
    close_html_page($sizep_fh);
    $sizep_fh = open_html_page('ifssize');
    print { $index_page_fh } "<a href='ifssize.html'>Every iteration sizes per filesystem</a><br />\n";
    foreach my $fs_name ( keys %{$bench_data->{'iterations'}{'size'}})
    {
	foreach my $job_name ( keys %{$bench_data->{'iterations'}{'size'}{$fs_name}})
	{
	    for(my$i=0;$i<$iterations;$i++)
	    {
		my $caption = sprintf("Iteration %d. Filesystem %s. %s", $i+1, $fs_name, $bench_data->{'captions'}{$job_name});
		my $img_filename = draw(
					    'hbars',
					    sprintf("iterations_fssizes_%s_%s_%d", $fs_name, $job_name, $i),
					    $caption,
					    'Names',
					    'Bytes',
					    $bench_data->{'captions'}{'size'}{$fs_name}{'names'},
					    $bench_data->{'iterations'}{'size'}{$fs_name}{$job_name}{'values'}[$i]
					    );
		post_image($sizep_fh, $img_filename, $caption);
	    }
	}
    }
    close_html_page($sizep_fh);
    #@{$bench_data->{'iterations'}{$type}{$fs_name}{$job_name}{'same_values'}{$size_part_name}}[$iteration] = $size_data->{$size_part_name};
    $sizep_fh = open_html_page('isamesize');
    print { $index_page_fh } "<a href='isamesize.html'>Every iteration sizes per type</a><br />\n";
    foreach my $fs_name ( keys %{$bench_data->{'iterations'}{'size'}})
    {
	foreach my $job_name ( keys %{$bench_data->{'iterations'}{'size'}{$fs_name}} )
	{
	    foreach my $size_part ( keys %{$bench_data->{'iterations'}{'size'}{$fs_name}{$job_name}{'same_values'}} )
	    {
		for(my$i=0;$i<$iterations;$i++)
		{
		    my $caption = sprintf("Iteration %d. Filesystem %s. Size type %s. %s", $i+1, $fs_name, $size_part, $bench_data->{'captions'}{$job_name});
		    my $img_filename = draw(
					    'lines',
					    sprintf("iterations_samesizes_%s_%s_%d", $fs_name, $job_name, $i),
					    $caption,
					    'Names',
					    'Bytes',
					    'Iteration',
					    $bench_data->{'iterations'}{'size'}{$fs_name}{$job_name}{'same_values'}{$size_part}
					    );
		    post_image($sizep_fh, $img_filename, $caption);
		}
	    }
	}
    }
    close_html_page($sizep_fh);
}

sub post_image
{
    my ($page_fh, $img_filename, $caption, $description) = @_[0..3];
    $description = defined($description) ? sprintf("<tr><td><code>%s</code></td></tr>", $description) : ' ';
    print { $page_fh } <<_;
<table align='center'>
<tr><th>${caption}</th></tr>
<tr><td><img src='${img_filename}' alt='${caption}' /></td></tr>
${description}
</table>
<br />
_
}

sub open_html_page
{
    my $filename = $_[0];
    my $fh;
    open($fh, sprintf(">%s/%s.html", $report_path, $filename));
    print { $fh } <<_;
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" 
   "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">
<head>
    <style type='text/css'>
	body { text-align: center; }
	table, td, tr { border-width: 1px; border-style: solid; }
	th { background-color: lightgray; }
    </style>
    <title>Filesystem test results graphs</title>
</head>
<body>
_
    return $fh;
}

sub close_html_page
{
    my $fh = $_[0];
    print { $fh } '</body></html>';
    close($fh);
}

sub draw
{
    my ($img_type, $filename_part, $title, $x_title, $y_title, $headers, $data) = @_[0..6];
    my $img_filename = sprintf("%s_%s_%s.png", $filename_part, $x_title, $y_title);
    print(".");
    #syswrite(STDOUT, ".", 1);
    my $items = scalar(@$data);
    if(ref($headers) ne "ARRAY") { my $n = $headers; $headers = []; for(my $i=1; $i<=$items; $i++) { push(@$headers, "${n} ${i}"); } }
#    my $colors;
#    foreach my $clr (split(/:/,`$gg -s=00FF00 -e=0000FF -S=$items -t=rcw -S=$items -l`))
#    {
#	push(@$colors, add_colour('#'.$clr));
#    }
    #print Dumper $colors;
    my $graph;
    if($img_type eq 'hbars')
    {
	$graph = GD::Graph::bars->new(800, 100+($items*30));
    	$graph->set	(
		    y_label     => $y_title,
		    x_label     => $x_title,
		    title	=> $title,
#		    dclrs	=> $colors,
		    dclrs	=> ['dbrown'],
		    #bar_width => 5,
		    bar_spacing => 2,
		    long_ticks => 0,
		    show_values => 1,
		    rotate_chart => 1
		) or die $graph->error;
    }
    elsif($img_type eq 'lines')
    {
	$graph = GD::Graph::lines->new(400, 300);
	$graph->set	(
		    y_label     => $y_title,
		    x_label     => $x_title,
		    title	=> $title,
		    dclrs	=> ['dbrown'],
		    show_values => 1,
		    line_width	=> 3
		) or die $graph->error;
    }
    #$graph->set_title_font('gdMediumBoldFont', 18);
    #$graph->set_legend_font('gdMediumBoldFont');
    #$graph->set_legend_font( ['verdana', 'arial', 'gdMediumBoldFont'], 12);
    #$graph->set_legend(@$headers);
    my @arr = ($headers, $data);
    my $img = $graph->plot(\@arr) or die $graph->error;
    
    my $ih; 
    open($ih, sprintf(">%s/%s", $report_path, $img_filename)) or die $!;
    binmode($ih);
    print { $ih } $img->png;
    close($ih);
    return $img_filename;
}

main();

